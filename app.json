{
  "expo": {
    "name": "Save My Spot",
    "slug": "save-my-spot",
    "version": "1.0.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.savemyspot.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Save your current location as a spot."
      }
    },
    "android": {
      "package": "com.savemyspot.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    }
  }
}
